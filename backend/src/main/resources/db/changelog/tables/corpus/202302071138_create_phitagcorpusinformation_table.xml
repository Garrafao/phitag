<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="202302071138" author="serge">

        <createTable tableName="phitagcorpusinformation">
            <column name="id" type="varchar">
                <constraints primaryKey="true" unique="true" nullable="false" />
            </column>

            <column name="title" type="varchar"></column>

            <column name="author" type="varchar"></column>

            <column name="date" type="int"></column>

            <column name="language" type="varchar"></column>

            <column name="resource" type="varchar">
                <constraints nullable="false" />
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>